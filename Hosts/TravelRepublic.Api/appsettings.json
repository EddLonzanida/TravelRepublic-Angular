{
  "ConnectionStrings": {
    "MainDbConnectionString": "Server=.;Database=TravelRepublic;MultipleActiveResultSets=true;Integrated Security=True"
  },
  "IntellisenseCount": 15,
  "PageSize": 10,
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "RateLimits": [
    {
      "Endpoint": "*",
      "Limit": 1000,
      "Period": "5m"
    },
    {
      "Endpoint": "*",
      "Limit": 200,
      "Period": "10s"
    }
  ],
  "WhiteList": [ "http://localhost:", "http://localhost:8081" ]
}
