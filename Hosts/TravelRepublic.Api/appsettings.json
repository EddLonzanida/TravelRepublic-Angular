{
  "ApplicationSettings": {
    "IntellisenseCount": 15,
    "PageSize": 10,
    "WhiteList": [ "http://localhost:8080", "http://localhost:8081" ]
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "RateLimits": [
    {
      "Endpoint": "*",
      "Limit": 1000,
      "Period": "5m"
    },
    {
      "Endpoint": "*",
      "Limit": 200,
      "Period": "10s"
    }
  ]
}
